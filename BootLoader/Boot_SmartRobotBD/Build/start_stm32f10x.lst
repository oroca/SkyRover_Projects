 384:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c ****     *(vu16*)Address = Data;
 385:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c ****     /* Wait for last operation to be completed */
 386:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c ****     status = FLASH_WaitForLastOperation(ProgramTimeout);
 387:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c **** 
 388:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c ****     if(status != FLASH_BUSY)
 389:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c ****     {
 390:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c ****       /* if the program operation is completed, disable the PG Bit */
 391:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c ****       FLASH->CR &= CR_PG_Reset;
 392:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c ****     }
 393:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c ****   } 
 394:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c ****   /* Return the Program Status */
 395:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c ****   return status;
 396:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c **** }
