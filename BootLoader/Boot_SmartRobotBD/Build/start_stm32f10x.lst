 398:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c **** /*******************************************************************************
 399:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c **** * Function Name  : FLASH_ProgramOptionByteData
 400:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c **** * Description    : Programs a half word at a specified Option Byte Data address.
 401:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c **** * Input          : - Address: specifies the address to be programmed.
 402:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c **** *                    This parameter can be 0x1FFFF804 or 0x1FFFF806. 
 403:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c **** *                  - Data: specifies the data to be programmed.
 404:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c **** * Output         : None
 405:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c **** * Return         : FLASH Status: The returned value can be: FLASH_BUSY, 
 406:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c **** *                  FLASH_ERROR_PG, FLASH_ERROR_WRP, FLASH_COMPLETE or 
 407:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c **** *                  FLASH_TIMEOUT. 
 408:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c **** *******************************************************************************/
 409:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c **** FLASH_Status FLASH_ProgramOptionByteData(u32 Address, u8 Data)
 410:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c **** {
 411:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c ****   FLASH_Status status = FLASH_COMPLETE;
 412:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c **** 
 413:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c ****   /* Check the parameters */
 414:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c ****   assert_param(IS_OB_DATA_ADDRESS(Address));
 415:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c **** 
 416:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c ****   status = FLASH_WaitForLastOperation(ProgramTimeout);
 417:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c **** 
 418:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c ****   if(status == FLASH_COMPLETE)
 419:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c ****   {
 420:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c ****     /* Authorize the small information block programming */
 421:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c ****     FLASH->OPTKEYR = FLASH_KEY1;
 422:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c ****     FLASH->OPTKEYR = FLASH_KEY2;
 423:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c **** 
 424:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c ****     /* Enables the Option Bytes Programming operation */
 425:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c ****     FLASH->CR |= CR_OPTPG_Set; 
 426:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c ****     *(vu16*)Address = Data;
 427:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c ****     
 428:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c ****     /* Wait for last operation to be completed */
 429:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c ****     status = FLASH_WaitForLastOperation(ProgramTimeout);
 430:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c **** 
 431:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c ****     if(status != FLASH_BUSY)
 432:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c ****     {
 433:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c ****       /* if the program operation is completed, disable the OPTPG Bit */
 434:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c ****       FLASH->CR &= CR_OPTPG_Reset;
 435:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c ****     }
 436:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c ****   }    
 437:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c ****   /* Return the Option Byte Data Program Status */
 438:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c ****   return status;      
 439:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c **** }
 440:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c **** 
 441:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c **** /*******************************************************************************
 442:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c **** * Function Name  : FLASH_EnableWriteProtection
 443:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c **** * Description    : Write protects the desired pages
 444:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c **** * Input          : - FLASH_Pages: specifies the address of the pages to be 
 445:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c **** *                    write protected. This parameter can be:
 446:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c **** *                    - For STM32F10Xxx Medium-density devices (FLASH page size equal to 1 KB)
 447:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c **** *                       - A value between FLASH_WRProt_Pages0to3 and 
 448:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c **** *                         FLASH_WRProt_Pages124to127
 449:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c **** *                    - For STM32F10Xxx High-density devices (FLASH page size equal to 2 KB) 
 450:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c **** *                       - A value between FLASH_WRProt_Pages0to1 and
 451:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c **** *                         FLASH_WRProt_Pages60to61 or FLASH_WRProt_Pages62to255 
 452:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c **** *                       - FLASH_WRProt_AllPages
 453:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c **** * Output         : None
 454:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c **** * Return         : FLASH Status: The returned value can be: FLASH_BUSY, 
 455:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c **** *                  FLASH_ERROR_PG, FLASH_ERROR_WRP, FLASH_COMPLETE or 
 456:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c **** *                  FLASH_TIMEOUT.
 457:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c **** *******************************************************************************/
 458:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c **** FLASH_Status FLASH_EnableWriteProtection(u32 FLASH_Pages)
 459:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c **** {
 460:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c ****   u16 WRP0_Data = 0xFFFF, WRP1_Data = 0xFFFF, WRP2_Data = 0xFFFF, WRP3_Data = 0xFFFF;
 461:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c ****   
 462:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c ****   FLASH_Status status = FLASH_COMPLETE;
 463:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c ****   
 464:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c ****   /* Check the parameters */
 465:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c ****   assert_param(IS_FLASH_WRPROT_PAGE(FLASH_Pages));
 466:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c ****   
 467:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c ****   FLASH_Pages = (u32)(~FLASH_Pages);
 468:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c ****   WRP0_Data = (vu16)(FLASH_Pages & WRP0_Mask);
 469:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c ****   WRP1_Data = (vu16)((FLASH_Pages & WRP1_Mask) >> 8);
 470:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c ****   WRP2_Data = (vu16)((FLASH_Pages & WRP2_Mask) >> 16);
 471:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c ****   WRP3_Data = (vu16)((FLASH_Pages & WRP3_Mask) >> 24);
 472:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c ****   
 473:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c ****   /* Wait for last operation to be completed */
 474:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c ****   status = FLASH_WaitForLastOperation(ProgramTimeout);
 475:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c ****   
 476:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c ****   if(status == FLASH_COMPLETE)
 477:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c ****   {
 478:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c ****     /* Authorizes the small information block programming */
 479:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c ****     FLASH->OPTKEYR = FLASH_KEY1;
 480:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c ****     FLASH->OPTKEYR = FLASH_KEY2;
 481:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c ****     FLASH->CR |= CR_OPTPG_Set;
 482:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c **** 
 483:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c ****     if(WRP0_Data != 0xFF)
 484:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c ****     {
 485:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c ****       OB->WRP0 = WRP0_Data;
 486:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c ****       
 487:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c ****       /* Wait for last operation to be completed */
 488:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c ****       status = FLASH_WaitForLastOperation(ProgramTimeout);
 489:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c ****     }
 490:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c ****     if((status == FLASH_COMPLETE) && (WRP1_Data != 0xFF))
 491:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c ****     {
 492:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c ****       OB->WRP1 = WRP1_Data;
 493:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c ****       
 494:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c ****       /* Wait for last operation to be completed */
 495:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c ****       status = FLASH_WaitForLastOperation(ProgramTimeout);
 496:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c ****     }
 497:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c **** 
 498:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c ****     if((status == FLASH_COMPLETE) && (WRP2_Data != 0xFF))
 499:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c ****     {
 500:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c ****       OB->WRP2 = WRP2_Data;
 501:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c ****       
 502:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c ****       /* Wait for last operation to be completed */
 503:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c ****       status = FLASH_WaitForLastOperation(ProgramTimeout);
 504:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c ****     }
 505:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c ****     
 506:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c ****     if((status == FLASH_COMPLETE)&& (WRP3_Data != 0xFF))
 507:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c ****     {
 508:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c ****       OB->WRP3 = WRP3_Data;
 509:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c ****      
 510:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c ****       /* Wait for last operation to be completed */
 511:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c ****       status = FLASH_WaitForLastOperation(ProgramTimeout);
 512:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c ****     }
 513:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c ****           
 514:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c ****     if(status != FLASH_BUSY)
 515:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c ****     {
 516:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c ****       /* if the program operation is completed, disable the OPTPG Bit */
 517:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c ****       FLASH->CR &= CR_OPTPG_Reset;
 518:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c ****     }
 519:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c ****   } 
 520:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c ****   /* Return the write protection operation Status */
 521:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c ****   return status;       
 522:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c **** }
 523:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c **** 
 524:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c **** /*******************************************************************************
 525:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c **** * Function Name  : FLASH_ReadOutProtection
 526:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c **** * Description    : Enables or disables the read out protection.
 527:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c **** *                  If the user has already programmed the other option bytes before 
 528:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c **** *                  calling this function, he must re-program them since this 
 529:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c **** *                  function erases all option bytes.
 530:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c **** * Input          : - Newstate: new state of the ReadOut Protection.
 531:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c **** *                    This parameter can be: ENABLE or DISABLE.
 532:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c **** * Output         : None
 533:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c **** * Return         : FLASH Status: The returned value can be: FLASH_BUSY, 
 534:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c **** *                  FLASH_ERROR_PG, FLASH_ERROR_WRP, FLASH_COMPLETE or 
 535:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c **** *                  FLASH_TIMEOUT.
 536:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c **** *******************************************************************************/
 537:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c **** FLASH_Status FLASH_ReadOutProtection(FunctionalState NewState)
 538:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c **** {
 539:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c ****   FLASH_Status status = FLASH_COMPLETE;
 540:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c **** 
 541:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c ****   /* Check the parameters */
 542:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c ****   assert_param(IS_FUNCTIONAL_STATE(NewState));
 543:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c **** 
 544:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c ****   status = FLASH_WaitForLastOperation(EraseTimeout);
 545:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c **** 
 546:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c ****   if(status == FLASH_COMPLETE)
 547:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c ****   {
 548:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c ****     /* Authorizes the small information block programming */
 549:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c ****     FLASH->OPTKEYR = FLASH_KEY1;
 550:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c ****     FLASH->OPTKEYR = FLASH_KEY2;
 551:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c **** 
 552:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c ****     FLASH->CR |= CR_OPTER_Set;
 553:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c ****     FLASH->CR |= CR_STRT_Set;
 554:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c **** 
 555:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c ****     /* Wait for last operation to be completed */
 556:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c ****     status = FLASH_WaitForLastOperation(EraseTimeout);
 557:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c **** 
 558:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c ****     if(status == FLASH_COMPLETE)
 559:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c ****     {
 560:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c ****       /* if the erase operation is completed, disable the OPTER Bit */
 561:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c ****       FLASH->CR &= CR_OPTER_Reset;
 562:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c **** 
 563:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c ****       /* Enable the Option Bytes Programming operation */
 564:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c ****       FLASH->CR |= CR_OPTPG_Set; 
 565:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c **** 
 566:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c ****       if(NewState != DISABLE)
 567:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c ****       {
 568:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c ****         OB->RDP = 0x00;
 569:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c ****       }
 570:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c ****       else
 571:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c ****       {
 572:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c ****         OB->RDP = RDP_Key;  
 573:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c ****       }
 574:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c **** 
 575:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c ****       /* Wait for last operation to be completed */
 576:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c ****       status = FLASH_WaitForLastOperation(EraseTimeout); 
 577:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c ****     
 578:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c ****       if(status != FLASH_BUSY)
 579:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c ****       {
 580:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c ****         /* if the program operation is completed, disable the OPTPG Bit */
 581:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c ****         FLASH->CR &= CR_OPTPG_Reset;
 582:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c ****       }
 583:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c ****     }
 584:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c ****     else 
 585:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c ****     {
 586:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c ****       if(status != FLASH_BUSY)
 587:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c ****       {
 588:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c ****         /* Disable the OPTER Bit */
 589:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c ****         FLASH->CR &= CR_OPTER_Reset;
 590:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c ****       }
 591:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c ****     }
 592:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c ****   }
 593:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c ****   /* Return the protection operation Status */
 594:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c ****   return status;      
 595:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c **** }
 596:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c ****   	
 597:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c **** /*******************************************************************************
 598:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c **** * Function Name  : FLASH_UserOptionByteConfig
 599:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c **** * Description    : Programs the FLASH User Option Byte: IWDG_SW / RST_STOP /
 600:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c **** *                  RST_STDBY.
 601:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c **** * Input          : - OB_IWDG: Selects the IWDG mode
 602:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c **** *                     This parameter can be one of the following values:
 603:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c **** *                     - OB_IWDG_SW: Software IWDG selected
 604:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c **** *                     - OB_IWDG_HW: Hardware IWDG selected
 605:../LIB_HW/Driver/LIB_ST32/stm32f10x_flash.c **** *                  - OB_STOP: Reset event when entering STOP mode.
